{
  "name": "@aureooms/js-collections-counter",
  "description": "Python's counter data structure for JavaScript",
  "version": "5.0.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-collections-counter/issues"
  },
  "dependencies": {
    "@aureooms/js-collections-dict": "^4.0.0",
    "@aureooms/js-compare": "^1.4.5",
    "@aureooms/js-error": "^5.0.0",
    "@aureooms/js-heapq": "^3.1.0"
  },
  "devDependencies": {
    "@aureooms/js-itertools": "5.0.1",
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.13.10",
    "@babel/register": "7.13.8",
    "ava": "3.15.0",
    "coveralls": "3.1.1",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "7.4.0",
    "nyc": "15.1.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "http://aureooms.github.io/js-collections-counter/",
  "keywords": [
    "collection",
    "collections",
    "count",
    "counter",
    "javascript",
    "js"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-collections-counter.git",
    "type": "git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  }
}
